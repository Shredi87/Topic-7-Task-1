{
  "name": "javascript-airbnb",
  "version": "1.0.0",
  "scripts": {
    "lint": "markdownlint --config linters/.markdownlint.json README.md */README.md",
    "speller": "yaspeller --config linters/.yaspeller.json ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonidlebedev/javascript-airbnb.git"
  },
  "keywords": [
    "style guide",
    "lint",
    "airbnb",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "es2018",
    "react",
    "jsx"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonidlebedev/javascript-airbnb/issues"
  },
  "homepage": "https://github.com/leonidlebedev/javascript-airbnb",
  "devDependencies": {
    "eslint": "^8.41.0",
    "husky": "7.0.4",
    "markdownlint-cli": "0.30.0",
    "yaspeller": "8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run speller"
    }
  },
  "main": ".eslintrc.js",
  "dependencies": {
    "milsymbol": "^2.2.0"
  },
  "author": "",
  "description": ""
}
